	public static String findDay(int month, int day, int year) {

		String input = String.valueOf(month)+" "+String.valueOf(day)+" "+String.valueOf(year);
		Date date = null;
        try {
            date = new SimpleDateFormat("MM-DD-YYYY").parse(input);
        } catch (ParseException e) {
            e.printStackTrace();
        }
        return new SimpleDateFormat("EEEE", Locale.ENGLISH).format(date);
    }
	
	public static void main(String[] args) {
	
		System.out.println(findDay(10, 20, 2020));
		//Test t = new Test();
		//EOF();
	}
	
	static void EOF(){
		
		int count = 0;
		
		while(s.hasNextLine())
			System.out.println(++count+" "+s.nextLine());
	}
	
	
